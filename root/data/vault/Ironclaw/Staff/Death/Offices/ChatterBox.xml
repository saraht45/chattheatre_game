      <Core:Property property="merry:lib:time/dusk">
         X[M] EmitIn(\$this, "It's like totally evening now. We are inside and there are no windows, we must be psychic or maybe psychotic.");                   
return TRUE;
      </Core:Property>
      <Core:Property property="merry:lib:time/midday">
         X[M] EmitIn(\$this, "It's midday. What the hell is wrong with you? It's called noon, people!");                   
return TRUE;
      </Core:Property>
      <Core:Property property="merry:lib:time/midnight">
         X[M] EmitIn(\$this, "Okay, so it just turned midnight. How the hell can you even tell from here? Is your huge cogwheel clock strapped to your thigh or something?");                   
return TRUE;
      </Core:Property>
      <Core:Property property="merry:lib:time/morning">
