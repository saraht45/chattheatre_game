return TRUE;
      </Core:Property>
      <Core:Property property="merry:react-post:climb-dob">
         X[M] if (NRefDetail(\$dob[0]) == "wall-east")  \{         
EmitIn(\$this, Describe(\$actor) +" reaches the top and disappears.", \$actor);   
EmitTo(\$actor, "You reach the top and step over the side."); 
\$dest = \$\{Ironclaw:Staff:Death:Offices:ClassRoom\};   
Set(\$actor, "base:environment", \$dest);   
EmitIn(\$dest, Describe(\$actor) + " climbs out of " + Describe(\$this) + ".", \$actor);  
EmitTo(\$actor, "You climb out of " +Describe(\$this)+ " and stand up."); 
\$proxy = NewNRef( \$dest, "box-chatters" );   
    \$actor."base:stance" = 5;  
    \$actor."base:stancestring" = "standing";  
    \$actor."base:preposition" = 8;  
    \$actor."base:proximity" = \$proxy;  
(\$delay, 1);  
\$glance = UnSAM(Get(\$dest, "details:default:description:glance"));        
EmitTo(\$actor, Str(\$glance));  
\}         
return TRUE;
      </Core:Property>
