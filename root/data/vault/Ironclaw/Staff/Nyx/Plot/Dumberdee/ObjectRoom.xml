         X[M] \$delay(1, TRUE, "030f");

\$magic = Int(\$actor."skill:academics:magic") + Int(\$actor."skill:career:scholar");

if(\$magic \> 5) \{
  EmitTo(\$actor, "The etchings on the walls appear to be magical in nature. They don't seem to have been carved in, but rather exist as a raised pattern that doesn't damage the structure itself.");
\} else if(\$magic \> 8) \{
  EmitTo(\$actor, "To reach the treasure whisper to the guardians.");
\} else if(\$scholar \> 5) \{
