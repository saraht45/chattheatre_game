         X[M] if(!\$this."tooth:quantity") \{ \$this."tooth:quantity" = 10; \};      
/* check if tooth is available */      
if(\$this."trait:tooth") \{EmitTo(\$actor, "Someone already removed all the teeth from Lefty."); return FALSE; \};      
/* emit removing the tooth from Lefty's head */      
EmitTo(\$actor, "You reach into the mouth of the severed head of Lefty and grab a single tooth. You pull on it until you successfully manage to yank it from the gums."); EmitIn(\$actor."base:environment", Describe(\$actor)+" reaches into the mouth of the severed head of Lefty and successfully manages to yank a tooth from the gums.", \$actor);      
/* spawn the tooth, set counter */      
\$tooth = Spawn(\$\{Ironclaw:Staff:Ditto:Liss:tooth\});      
\$tooth."base:environment" = \$actor;      
\$this."tooth:quantity" -= 1;      
if(\$this."tooth:quantity" == 0) \{\$this."trait:tooth" = TRUE; return FALSE; \}      
return TRUE;
