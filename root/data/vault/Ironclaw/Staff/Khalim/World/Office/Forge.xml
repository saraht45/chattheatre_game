    <Core:Properties>
      <Core:Property property="merry:lib:start_my_loop">
         X[M] if (this."my_loop:identifier") Call( this, "stop_my_loop" );

EmitIn(this, "Loop Starting!");

this."my_loop:identifier" = Every( "my_loop", 60 );

/*this."my_loop:identifier" = "test";*/
      </Core:Property>
      <Core:Property property="merry:lib:stop_my_loop">
         X[M] if (this."my_loop:identifier" )
\{
  /* No loop found */
\}

EmitIn(this, "Loop Stopping!");

Stop( this."my_loop:identifier" );
this."my_loop:identifier" = nil;
      </Core:Property>
      <Core:Property property="merry:react:poke-dob">
