      <Core:Property property="merry:react-post:coo-iob">
         X[M] EmitTo(\$actor, Describe(\$this)+ " blinks up at you before cheerfully chirping back at you.");
EmitIn(\$actor."base:environment", Describe(\$this)+ " cheerfully chirps back at "+Describe(\$actor)+".", \$actor);
return TRUE;
      </Core:Property>
      <Core:Property property="merry:react-post:name-dob">
         X[M] if(this."trait:name") \{
  EmitTo(\$actor, Describe(this)+" already has a name. Sheesh.");
  return TRUE;
\}

\$name = capitalize(\$evoke[1..strlen(\$evoke)-2]);
/* If we have any punctuation at the end of the command, remove it */
if(!((\$name[strlen(\$name)-1] \>= 97) \&\& (\$name[strlen(\$name)-1] \<= 122)) \&\& !((\$name[strlen(\$name)-1] \>= 65) \&\& (\$name[strlen(\$name)-1] \<= 90)))
  \$name = \$name[0..strlen(\$name)-2];
/* Only take the first word of the name */
sscanf(\$name, "%s %*s", \$name);

/* Tell the actor that this was successful */
EmitTo(\$actor, Describe(this)+" will now respond to "+\$name+".");

/* Set the name */
this."trait:name" = \$name;
/* set the sname */
Set(this, "details:default:sname:"+lower_case(\$name), 1);


return TRUE;
      </Core:Property>
      <Core:Property property="merry:react-post:talk-iob">
         X[M] EmitTo(\$actor, Describe(\$this)+ " chirps back to you, oblivious of what you actually are saying to it.");
EmitIn(\$actor."base:environment", Describe(\$this)+ " chirps back at " +Describe(\$actor)+" obliviously.", \$actor);
return TRUE;
      </Core:Property>
