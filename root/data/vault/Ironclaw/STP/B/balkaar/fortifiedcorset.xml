\$observ = ironclaw::roll(\$roller: \$actor, \$skills: (\{ "mental:observation" \}), \$bonus: "trait:mind"); 
 
/*Emitting according to observation roll*/ 
if (\$observ \< 4) \{ 
   EmitTo(\$actor, "Just because it shows cleavage doesn't mean you should stare."); 
\} else if (\$observ \< 9) \{ 
   EmitTo(\$actor, "Something draws your attention to the undercorset; perhaps the slightly rigid bumps upon its surface, but you can't seem to devise anything from it."); 
\} else if (\$observ \< 11) \{ 
   EmitTo(\$actor, "This is obviously an uncommon undercorset, allowing for less flexibility than most others you have seen. The panels seem to remain fixed regardless of the wearers movements."); 
\} else if (\$observ == 11) \{ 
   EmitTo(\$actor, "You can determine that this undercorset is hiddenly lined with metal plates; a fashionable garment of defense."); 
\} else if (\$observ == 12) \{ 
   EmitTo(\$actor, "This undercorset is a hidden defense system for the torso, and is secretly fortified with metal plates which show here and there upon the surface of the garment via slightly rigid bumps."); 
\} 
 
 
return TRUE;
