  function sanitize_charname(str) \\\{
      var i;
      i = str.indexOf(' ');
      while (i != -1) \\\{
          str = str.substring(0, i) + '_' +
                str.substring(i + 1);
          i = str.indexOf(' ');
      \\\}
      i = str.indexOf(':');
      while (i != -1) \\\{
          str = str.substring(0, i) + '_' +
                str.substring(i + 1);
          i = str.indexOf(' ');
      \\\}
      return str;
