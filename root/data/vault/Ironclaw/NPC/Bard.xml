      <Core:Property property="merry:lib:add_question">
         X[M] /* Add this question to the trivia bot's list of questions */
\$actor."trivia:addquestion" = nil;

\$\{IronClaw:Lib:Trivia\}."trivia:questions" \|= (\{ (\{ \$par["question"], \$par["answer"], \$par["hint"] \}) \});
EmitTo(\$actor, "Question added!");
return TRUE;
      </Core:Property>
      <Core:Property property="merry:lib:no_question">
         X[M] /* We don't want to add this question. In fact, delete it from the player */
\$actor."trivia:addquestion" = nil;
return TRUE;
      </Core:Property>
      <Core:Property property="merry:react-post:answer-dob">
         X[M] /* Are we adding a question to the trivia bot? */
if(!\$evoke) return TRUE;

if(!\$actor."trivia:addquestion") return TRUE;

\$answer = \$evoke[1..strlen(\$evoke)-3];
/* If answer is one word, the hint is the number of letters, otherwise it is the number of words */
\$wordcount = sizeof(explode(\$answer, " "));
if(\$wordcount \> 1) \{
  \$hint = capitalize(desc_cardinal(\$wordcount))+" words.";
\} else \{
  if(\$answer == "yes" \|\| \$answer == "no")\{
    \$hint = "Yes or No?";
  \} else \{
    \$hint = capitalize(desc_cardinal(strlen(\$answer)))+" letters.";
  \}
\}

EmitTo(\$actor, "Question: "+\$actor."trivia:addquestion"+" "+\$hint);
EmitTo(\$actor, "Answer: "+\$answer);
interaction::question(\$question: "Do you want to add this question to "+Describe(\$this)+"'s list of trivia questions?", \$expires: 80, \$env: "TRUE", \$yes_ob: \$this, \$no_ob: \$this, \$expire_ob: \$this, \$yes_fun: "add_question", \$no_fun: "no_question", \$expire_fun: "no_question", \$par: ([ "question": capitalize(\$actor."trivia:addquestion"), "answer": lower_case(\$answer), "hint": \$hint]));
return TRUE;
      </Core:Property>
      <Core:Property property="merry:react-post:ask-dob">
         X[M] /* Are we adding a question to the trivia bot? */
if(!\$evoke) return TRUE;

\$actor."trivia:addquestion" = \$evoke[1..strlen(\$evoke)-3]+"?";
return TRUE;
      </Core:Property>
