      <Core:Property property="merry:react-post:feed-dob">
         X[M] if (NRefDetail(\$target) == "ducks") \{   
    /* describe the ducks being fed */      
    EmitIn(\$actor."base:environment", "The ducks flock eagerly to the offering, happily bill munching on whatever is tossed their way.");       
\}

if (NRefDetail(\$target) == "swans") \{    
    /* describe the swans being fed */      
    EmitIn(\$actor."base:environment", "The swans delicately float in the direction of the food, not seeming to be in any hurry. With their long necks they gather and dine on the scraps in their path before setting to graze in the grass.");       
\}

return TRUE;
      </Core:Property>
      <Core:Property property="merry:react-pre:gather-dob">
         X[M] if (NRefDetail(\$target) == "flood_water")       
  \{       
\$object = Spawn(\$\{Ironclaw:Props:Plot:Tsunami:Scallop\}); \$object."base:environment" = \$actor;       
EmitTo(\$actor, "You gather a sea creature from the flood water.");       
EmitIn(\$actor."base:environment", Describe(\$actor)+" gathers a sea creature from the flood water.", \$actor);       
       
return FALSE;       
\} else \{       
return TRUE;       
\}
      </Core:Property>
      <Core:Property property="merry:react-pre:skate-dob">
         X[M] if (NRefDetail(\$target) == "pond")       
\{
  \$bonus = 0;
  if (\$actor."appearance:feettype" == "hooves")
  \{
    \$bonus = -1;
  \}
  if (\$actor."trait:skatingbonus")
  \{
    \$bonus = \$actor."trait:skatingbonus";
  \}

  \$roll = (random(5) + random(5) + random(5)) - \$bonus;
  \$agi = \$actor."trait:agility";

  if (\$agi - \$roll \>= 3)
  \{
    /* Very successful skate attempt */
    EmitTo(\$actor, "You gracefully glide across the frozen pond on your " + \$actor."appearance:feettype" + ", making it seem effortless!");
    EmitIn(\$actor."base:environment", Describe(\$actor) + " gracefully glides across the frozen pond on " + \$actor."trait:possessive" + " " + \$actor."appearance:feettype" + ", making it seem effortless!", \$actor);
  \}
  else if (\$agi - \$roll \>= 1)
  \{
    /* Successful skate attempt */
    EmitTo(\$actor, "You slide across the frozen pond on your " + \$actor."appearance:feettype" + ".");
    EmitIn(\$actor."base:environment", Describe(\$actor) + " slides across the frozen pond on " + \$actor."trait:possessive" + " " + \$actor."appearance:feettype" + ".", \$actor);
  \}
  else if (\$agi - \$roll \>= -1)
  \{
    /* Just passing skate attempt */
    EmitTo(\$actor, "You slide across the frozen pond, wobbling considerably, but barely manage to stay on your " + \$actor."appearance:feettype" + ".");
    EmitIn(\$actor."base:environment", Describe(\$actor) + " slides across the frozen pond, wobbling considerably, but barely manages to stay on " + \$actor."trait:possessive" + " " + \$actor."appearance:feettype" + ".", \$actor);
  \}
  else if (\$agi - \$roll \>= -3)
  \{
    /* Failed skate attempt */
    EmitTo(\$actor, "You try to slide across the frozen pond on your " + \$actor."appearance:feettype" + ", but you slip and fall.");
    EmitIn(\$actor."base:environment", Describe(\$actor) + " tries to slide across the frozen pond on " + \$actor."trait:possessive" + " " + \$actor."appearance:feettype" + ", but " + \$actor."trait:nominative" + " slips and falls.", \$actor);
  \}
  else
  \{
    /* Botched skate attempt */
    EmitTo(\$actor, "You try to slide across the frozen pond on your " + \$actor."appearance:feettype" + ", but you slip and fall on your " + \$actor."trait:butt" + " with an audible thud!");
    EmitIn(\$actor."base:environment", Describe(\$actor) + " tries to slide across the frozen pond on " + \$actor."trait:possessive" + " " + \$actor."appearance:feettype" + ", but " + \$actor."trait:nominative" + " slips and falls on " + \$actor."trait:possessive" + " " + \$actor."trait:butt" + " with an audible thud!", \$actor);
  \}

  return FALSE;       
\} else \{       
  return TRUE;       
\}
