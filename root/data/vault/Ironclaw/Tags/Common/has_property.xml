<object clone="/usr/SkotOS/obj/mersamtag" owner="tonyd">
  <SkotOS:MerSAMTag tag_name="has_property">
    <Core:PCProperties>
      <Core:Property property="merry:global:tag">
         X[M] /* add a or an to the start */
mixed value;

if(!\$(tag-content) \|\| \$(tag-content) == "") return "";
if(!\$property \|\| \$propert == "") return "";

if(\$target \&\& typeof(\$target) == T_OBJECT)
    \$this = NRefOb(\$target);

value = Get(\$this, \$property);

if(typeof(value) == T_ARRAY \&\& !sizeof(value)) return "";
else if(!value \|\| value=="") return "";

return \$(tag-content);
      </Core:Property>
