      <Core:Property property="merry:react:cut-dob">
         X[M] /* spawn the meat */
object *carvers;
int i;

carvers = Arr(\$this."trait:carved");
for (i = 0; i \< sizeof(carvers); i++) \{
  if(carvers[i] == \$actor) \{
    EmitTo(\$actor, "You can only have one piece of meat. It should fill you up.");
    return FALSE;
  \}
\}

EmitTo(\$actor, "You carve a piece of meat from the body of the dray, cook it up, and season it.");
EmitIn(\$actor."base:environment", Describe(\$actor)+" carves a piece of meat from the body of the dray, cooks it up, and seasons it.", \$actor);
   \$feather = Spawn(\$\{IronClaw:Props:Food:Houses:Bisclavret:SocialRecipe:DrayMeatball\});
   \$feather."base:environment" = \$actor;

\$this."trait:carved" = (Arr(\$this."trait:carved") - (\{ \$actor \})) + (\{ \$actor \});

return FALSE;
      </Core:Property>
      <Core:Property property="merry:react:skin-dob">
         X[M] /* spawn the fabric */
object *skinners;
int i;

skinners = Arr(\$this."trait:skinned");
for (i = 0; i \< sizeof(skinners); i++) \{
  if(skinners[i] == \$actor) \{
    EmitTo(\$actor, "You can only have one gathering of fabric. Kthnxbai.");
    return FALSE;
  \}
\}

EmitTo(\$actor, "You carefully skim some leather from the body of the dray.");
EmitIn(\$actor."base:environment", Describe(\$actor)+" carefully skins some leather from the body of the dray.", \$actor);
   \$feather = Spawn(\$\{Ironclaw:Animals:Wild:WildDrayLoot:Fabric\});
   \$feather."base:environment" = \$actor;

\$this."trait:skinned" = (Arr(\$this."trait:skinned") - (\{ \$actor \})) + (\{ \$actor \});

return FALSE;
      </Core:Property>
