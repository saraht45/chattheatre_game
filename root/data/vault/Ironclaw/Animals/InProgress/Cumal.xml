      <Core:Property property="merry:react:butcher-dob">
         X[M] /* Butchering the dray */
/* is the dray already dead? */
if(!\$this."death:dead") \{
 EmitTo(\$actor, "Butchering a living animal? That's pretty cruel. You might want to kill it first.");
 return FALSE;
\}

/* is there any left? */
if(!\$this."trait:quantity" \|\| \$this."trait:quantity" \<= 0) \{
 EmitTo(\$actor, "There are no good cuts remaining on "+Describe(\$this, nil, \$actor)+".");
 return FALSE;
\}

/* do they have a cleaver? */
if(!sizeof(Match(\$actor, "cleaver")) \&\& !sizeof(Match(\$actor, "knife"))) \{
 EmitTo(\$actor, "Usually drays are butchered with a cleaver or knife.");
 return FALSE;
\}

\$meat = Spawn(\$\{Allegory:Props:Food:DeleteMe:Cooking:Meat:SteakUncooked\});
\$meat."base:environment" = \$actor;
if(\$meat."base:environment" != \$actor) \{
 EmitTo(\$actor, "You can't spot any good cuts remaining on "+Describe(\$this, nil, \$actor)+".");
 Slay(\$meat);
 return FALSE;
\} else \{
 \$this."trait:quantity"--;
\}

EmitTo(\$actor, "You cut "+Describe(\$meat, nil, nil, STYLE_NONPOSS)+" from "+Describe(\$this, nil, \$actor)+".");
EmitIn(\$actor."base:environment", Describe(\$actor)+" cuts "+Describe(\$meat, nil, nil, STYLE_NONPOSS)+" from "+Describe(\$this, \$actor)+".");

return FALSE;
      </Core:Property>
      <Core:Property property="merry:react:kill-dob">
         X[M] /* Killing the dray */
/* is the dray already dead? */
if(\$this."death:dead") return TRUE;

/* do they have a cleaver? */
\$cleaver = Match(\$actor, "cleaver");

if(!sizeof(\$cleaver)) \{
 EmitTo(\$actor, "Usually drays are slaughtered with a cleaver.");
 return FALSE;
\}

EmitTo(\$actor, "You draw "+Describe(\$cleaver[0], nil, \$actor)+" across "+Describe(\$this, nil, \$actor)+"'s throat, slaughtering it.");
EmitTo(\$this, Describe(\$actor)+" draws "+Describe(\$cleaver[0], \$actor)+" across your throat, slaughtering you.");
EmitIn(\$actor."base:environment", Describe(\$actor)+" draws "+Describe(\$cleaver[0], \$actor)+" across "+Describe(\$this, \$actor)+"'s throat, slaughtering it.", \$actor, \$this);

\$this."death:dead" = 1;
\$this."details:default:sname:corpse" = 1;
\$this."details:default:pname:corpses" = 1;
\$this."details:default:adjective:dray" = 1;

return FALSE;
      </Core:Property>
      <Core:Property property="merry:react:skin-dob">
         X[M] /* Butchering the dray */
/* is the dray already dead? */
if(!\$this."death:dead") \{
 EmitTo(\$actor, "Skinning a living animal? That's pretty cruel. You might want to kill it first.");
 return FALSE;
\}

/* is there any left? */
if(\$this."trait:skinned") \{
 EmitTo(\$actor, "The good parts have already been removed from "+Describe(\$this, nil, \$actor)+".");
 return FALSE;
\}

/* do they have a knife? */
if(!sizeof(Match(\$actor, "knife"))) \{
 EmitTo(\$actor, "Usually drays are skinned with a knife.");
