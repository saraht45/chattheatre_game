         X[M] /* \$tmp = dump_value( args ); */      
/* EmitTo(\$actor, \$tmp); */      
      
/* check if they look at something */      
if(\$what) \{      
      
/* start walking through the \$whats and check for spiderweb and hidingspiders */      
\$sz = sizeof(\$what);      
for(\$i = 0; \$sz \> \$i; ++\$i) \{      
\$det = NRefDetail(\$what[\$i]);      
\$det = Str(\$det);      
      
/* if \$what includes web or spiders, emit something */      
if (\$det == "spiderwebs" \|\| \$det == "hidingspiders") \{     
/* set a value to determine output */      
\$tmp = TRUE;      
\} \} \}     
if(\$tmp) \{     
    
/* build the emits */    
    
/* describe the main spider */    
\$def = "A ";    
\$hue = oneof("deep ", "dark ", "light ", "faded ", "translucent ", "pearly ", "marbleized ", "pale ");    
\$color = oneof("red", "orange", "blue", "black", "brown", "yellow", "green", "purple", "grey", "gray", "rust", "copper", "silver", "gold", "white", "ecru", "tan", "pink", "amber", "amethyst", "crimson");    
\$spidey = " spider ";    
/* add a variation to spider */    
\$var = oneof("missing one eye ", "with long, spindly legs ", "with an hourglass symbol upon its back ", "bearing beady eyes ", "holding a stray fly in its fangs ", "sucking the juices from its newest victim ", "lazing on a strand of webbing ", "staring off into space ", "considering its next meal ");    
    
\$start = \$def + \$hue + \$color + \$spidey +\$var;    
    
/* give it the 1st action */    
    
\$act1 = oneof("spots you ", "notices you ", "ignores you ", "is oblivious to you ", "may have frowned at you ", "appears to be staring at you ", "can smell your blood " );    
    
/* connection */    
    
\$and = oneof("and ", "then ");    
    
/* give it the 2nd action verb */    
    
\$act2 = oneof("skitters ", "crawls ", "creeps ", "inches ", "climbs ");    
    
/* give it a place to go */    
    
\$to = oneof("into the darkness.", "up a strand of webbing.", "into a nearby hole.", "into a dark hole.", "on top of a rafter.", "along the ceiling.");    
    
\$end = \$act1 + \$and + \$act2 + \$to;    
    
/* finish the emit build */    
    
\$emit = \$start + \$end;    
    
/* send emit */     
EmitTo(\$actor, \$emit);     
/* reset value */     
\$tmp = FALSE;     
\}     
     
return TRUE;
