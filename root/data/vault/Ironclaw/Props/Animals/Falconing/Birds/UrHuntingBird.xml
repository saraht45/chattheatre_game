      <Core:Property property="merry:lib:ingame:loop">
         X[M] if(this."ingame:looping") return TRUE; 
this."ingame:looping" = 1; 
while(this."ingame:looping") \{ 
  \$delay(60*15, TRUE, "e00d"); 
  if(!this."base:environment" \|\| !this.caller \|\| !this.caller."base:environment") \{ 
    if(this."base:environment") 
      EmitIn(this."base:environment", Describe(this)+" flies up and away."); 
    this.onhand = nil; 
    this."ingame:looping" = nil; 
    this."base:environment" = nil; 
  \} 
\} 
return TRUE;
      </Core:Property>
      <Core:Property property="merry:react-post:point-dob%falconry">
         X[M] if(!\$actor."falconry:glove" \|\| !\$actor."falconry:glove".bird \|\| \$actor."falconry:glove".bird != this) return TRUE; 
/* tells the bird to fly somewhere */ 
if(!\$(iob-2)) return TRUE; 
if(sizeof(Arr(\$(iob-2)))\>1) \{ 
  EmitTo(\$actor, Describe(\$this)+" considers you curiously."); 
  EmitIn(\$actor."base:environment", Describe(\$this)+" considers "+Describe(\$actor)+" curiously.", \$actor); 
  return TRUE; 
\} 
 
if(!this."base:environment") return TRUE; 
 
EmitIn(this."base:environment", Describe(this)+" launches itself into the air."); 
this."base:proximity" = Arr(\$(iob-2))[0]; 
this."base:prepositionstring" = "on"; 
this.onhand = nil; 
 
\$delay(1, TRUE, "3d5b"); 
EmitTo(NRefOb(Arr(\$(iob-2))[0]), Describe(this)+" swoops to you."); 
EmitIn(this."base:environment", Describe(this)+" swoops to "+Describe(\$(iob-2))+".", NRefOb(Arr(\$(iob-2))[0])); 
\$delay(1, TRUE, "9e4e"); 
EmitTo(NRefOb(Arr(\$(iob-2))[0]), Describe(this)+" perches on "+Describe(\$(iob-2), nil, NRefOb(Arr(\$(iob-2))[0]))+"."); 
EmitIn(this."base:environment", Describe(this)+" perches on "+Describe(\$(iob-2), NRefOb(Arr(\$(iob-2))[0]))+".", NRefOb(Arr(\$(iob-2))[0])); 
 
 
return TRUE;
      </Core:Property>
      <Core:Property property="merry:react-post:release-dob%falconry">
         X[M] /* releases the bird to hunt */ 
if(!\$actor."falconry:glove" \|\| !\$actor."falconry:glove".bird \|\| \$actor."falconry:glove".bird != this) return TRUE; 
 
if(!\$at) \{ 
  /* release the bird to fly away */ 
  EmitIn(\$actor."base:environment", Describe(this)+" flies up and away."); 
  this."base:environment" = nil; 
  this.onhand = nil; 
\} 
return TRUE;
      </Core:Property>
