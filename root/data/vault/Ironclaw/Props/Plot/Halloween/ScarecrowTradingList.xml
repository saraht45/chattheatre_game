      <Core:Property property="merry:lib:ask_message">
         X[M] /* This is run if someone "asks" the trader */
\$response = oneof("What do you want?", "Do you think my gifts come without a cost?", "What can the scarecrow do for you?", "The harvest night approaches", "A blood moon rises", "The warm blood running through your veins smells so delicious", "You dare to ask me for gifts?", nil, nil, nil);

if(!random(3)) \{
  Social(\$seller, "turn", oneof("creakily", "ominously", "sharply"), \$response, "towards", \$buyer);
\} else if(!random(2)) \{
  Social(\$seller, oneof("glare", "gaze", "stare"), "down", \$response, "at", \$buyer);
\} else \{
  Social(\$seller, "balance", oneof("stiffly", "motionlessly"), \$response);
\}

return TRUE;
