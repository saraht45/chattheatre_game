      <Core:Property property="merry:react-post:examine-what">
         X[M] if(NRefDetail(\$what[0]) != "default") \{ return TRUE; \}

\$delay(1, TRUE, "e712");
\$heal = Int(\$actor."skill:career:healer") + Int(\$actor."skill:mental:first-aid");

if(\$heal \< 4) \{
   EmitTo(\$actor, "You find the 3 layers of the skin, as they build up layer by layer, you also note something odd about the lowest layer of the skin.");
\} else if (\$heal \> 24) \{
     EmitTo(\$actor, "Each of the layers of the skin tells you something on its own, the marks on the epidermis hint the constant scratching and irritation, though you do not see an exact reason that would make the skin's owner, scratch. The dermis shows thinning of the hair and the hypodermis has vesicles attached to it filled with amber colored fluid.");
\} else if (\$heal \> 14) \{
     EmitTo(\$actor, "You locate the three layers of the skin, the epidermis, the dermis and the hypodermis, all having some deformities. You particularly note the vesicles attached to the hypodermis and the thinning of the hair.");
\}else if (\$heal \>= 4) \{
     EmitTo(\$actor, "You locate the 3 layers of the skin, the epidermis, the dermis and the hypodermis, also you note some oddities about the hair and the lowest layer of the skin.");
\}

return TRUE;
