      <Core:Property property="merry:act:start">
         X[M] Set(this, "trait:noise:tick_id", Every("tick", 60));
      </Core:Property>
      <Core:Property property="merry:timer:tick">
         X[M] object env; 
env = \$this."base:environment"; 
switch(random(9)) \{ 
    case 0: 
      EmitIn(env, "A pair of acrobats "+ oneof("practicedly", "effortlessly")+ " walk across the length of a thin tight-rope with graceful precision."); 
      break; 
    case 1: 
      EmitIn(env, "A clown "+ oneof("blows up", "inflates", "breathes life into")+ " "+ oneof("a bright pink balloon", "a sunshine yellow balloon", "a lavender-colored balloon")+ " with a  "+ oneof("talented turn", "light pursing", "persuasive puff", "playful pffbbtt")+ " of his lips."); 
      break; 
    case 2: 
      EmitIn(env, "The juggler smoothly transitions into an "+ oneof("impressive", "awe-inspiring")+ " trick of the hands. The "+ oneof("multi-colored balls", "shiny batons")+ " whirl "+ oneof("climactically", "dramatically", "frenziedly")+ " in the air before returning safely to her palms."); 
      break; 
    case 3: 
      EmitIn(env, "The hooper endlessly circles his limbs with a variety of ring-like objects. It is apparent he has no intention of ever breaking rhythm so long as there is a crowd."); 
      break; 
    case 4: 
      EmitIn(env, "A "+ oneof("large yellow lizard with spots", "small orange lizard with stripes", "variegated medium-sized lizard")+ " snaps it's tongue outwardly, capturing a small crowd of flies. It eventually opens up at the behest of the lizard trainer releasing the swarm in a heart-shaped pattern."); 
      break; 
    case 5: 
      EmitIn(env, "The pair of acrobats tumble and switch experiencedly from one trapeze to another."); 
      break; 
    case 6: 
      EmitIn(env, "The colorfully-painted clown wanders around telling jokes. You hear above the voices in the crowd, 'Why can't clown noses be twelve inches long?', 'Because then they would be a foot! Yuck, yuck.'"); 
      break; 
    case 7: 
      EmitIn(env, "The "+ oneof("constant", "incessant", "jovial", "gay", "pleased")+ " sounds from the "+ oneof("crowd", "audience", "onlookers", "observers")+ " permeate the air."); 
      break; 
    case 8: 
      EmitIn(env, "The "+ oneof("lizard trainer", "hooper", "clown")+ " smiles "+ oneof("pleasedly", "genuinely", "warmly")+ " at the faces in the "+ oneof("crowd", "audience")+ " before beginning with the next act."); 
      break; 
    default: 
      EmitIn(env, "Activity continues to occur in the background."); 
      break; 
\} 
return TRUE;
