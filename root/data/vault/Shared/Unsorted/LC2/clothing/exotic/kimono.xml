      <Core:Property property="merry:act:start">
         X[M] /* Randomly set color, print and material along with appropriate adjectives */    
    
string color, material, trim, print;    
    
\{   
  color     = oneof( "pink", "pure white", "midnight black", "peacock blue", "leaf green", "crimson red", "bright yellow", "autumnal orange" );   
  material  = oneof( "silk", "silk brocade", "silk crepe", "cotton", "satin weave"  );   
  trim = oneof( "black", "pristine white", "dark blue", "dark red" );   
  print = oneof( "floral", "bamboo", "pine tree", "plum blossom", "cherry blossom", "chrysanthemum flowers", "pine leaves", "japanese fan", "meandering stream", "iris flower"  );  
   
  \$this."trait:color" = color;    
  \$this."trait:material" = material;    
  \$this."trait:trim" = trim;    
  \$this."trait:print" = print;  
  
  Set(\$this, "detail:adjectives:" + color, TRUE);    
  Set(\$this, "detail:adjectives:" + material, TRUE);    
  Set(\$this, "detail:adjectives:" + trim, TRUE);    
  Set(\$this, "detail:adjectives:" + print, TRUE);  
    
  return TRUE;    
\}
