      <Core:Property property="merry:lib:handler:request-authority">
         X[M] \$env = \$this."base:environment";      
\$detail = \$this.gatedetail;    
\$minrank = \$this.minrank;   

\$allowedpeople = (\{ \});     
if(\$this."allowed:people") \{     
    \$allowedpeople = \$this."allowed:people";     
\}    
      
if(!member(\$detail, Get(\$env, "base:details"))) \{      
    return TRUE;      
\}      
      
if(NRefDetail(\$target) != \$detail) \{      
    return TRUE;      
\}      
      
/* are they following someone through? */       
if(\$actor.follow \&\& NRefOb(\$dest) == \$actor.follow."base:environment")       
  return TRUE;      
 
 
if(\$actor."trait:superbrief" == "Dominic" ) \{
    Social(\$this, "announce", "loudly", "His Majesty, the King.", "for", \$actor);
\} else if(\$actor."trait:superbrief" == "Mirielle" ) \{
    Social(\$this, "announce", "loudly", "Her Majesty, the Queen.", "for", \$actor);
\} else if(\$actor."trait:superbrief" == "Jacoba" ) \{
    Social(\$this, "announce", "loudly", "His Royal Highness, the Prince.", "for", \$actor);
\} else if(\$actor."trait:superbrief" == "Joliet" ) \{
    Social(\$this, "announce", "loudly", "Her Royal Highness, the Princess.", "for", \$actor);
\} else if(\$actor."trait:superbrief" == "Alonzo" ) \{
Social(\$this, "announce", "loudly", "His Highness, the Archduke Fortini.", "for", \$actor);
\} else if(\$actor."trait:superbrief" == "Emeraude" ) \{
    Social(\$this, "announce", "loudly", "The Royal Justicar, her Ladyship di Farnese.", "for", \$actor);
\} else if(\$actor."trait:superbrief" == "Eliseo" ) \{
    Social(\$this, "announce", "loudly", "The Grand Chamberlain, his Lordship di Farnese.", "for", \$actor);
\} else if(\$actor."trait:superbrief" == "Kilana" ) \{
    Social(\$this, "announce", "loudly", "The Mistress of the Household, her Ladyship di Corvasci.", "for", \$actor);
\} else if(\$actor."trait:superbrief" == "Selene" ) \{
    Social(\$this, "announce", "loudly", "The Captain of the Royal Guard, Dame di Cosenza.", "for", \$actor);
\} else if(member(\$actor, \$allowedpeople))\{
    Social(\$this, "bow", "neatly", "Welcome.", "to", \$actor);  
\} else \{      
    Social(\$this, "block", nil, "Halt. You are not allowed through.", nil, \$actor);      
    return FALSE;     
\}  
      
return TRUE;
