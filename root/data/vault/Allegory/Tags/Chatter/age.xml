<object clone="/usr/SkotOS/obj/mersamtag" owner="jominey">
  <SkotOS:MerSAMTag tag_name="age">
    <Core:PCProperties>
      <Core:Property property="merry:global:tag">
         X[M] int age;
string rstr;

age = Int(NRefOb(\$target)."trait:age");

if(age \<= 0) \{
    age = 20;
    NRefOb(\$target)."trait:age" = age;
\}

if(age \<= 29) \{
    rstr = "young";
\} else if(age \> 29 \&\& age \<= 39) \{
    return "a";
\} else if(age \> 39 \&\& age \<= 49) \{
    rstr = "middle-aged";
\} else if(age \> 49 \&\& age \<= 59) \{
    rstr = "older";
\} else if(age \> 59) \{
    rstr = "elderly";
\}

return common::aan(\$what: rstr);
      </Core:Property>
