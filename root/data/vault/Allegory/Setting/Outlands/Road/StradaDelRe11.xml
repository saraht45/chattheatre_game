      <Core:Property property="merry:react-post:enter-into">
         X[M] /* Since this is a virtual room, close the door after the opener goes through */  
if(\$target) \{  
  \$detail = NRefDetail(\$target);  
  if(\$actor == Get(\$this, "details:"+\$detail+":opener"))  
    \$this."details:exit-west:exit:closed:local" = 1;  
\}  
  
return TRUE;
      </Core:Property>
      <Core:Property property="merry:react-post:open-what">
         X[M] /* If they are opening a virtual room, record who opened the door */  
if(\$target) \{  
  \$exit = Get(\$this, "details:"+NRefDetail(\$target)+":exit:dest");  
  if(\$exit \&\& NRefOb(\$exit).isvirtual)  
    Set(\$this, "details:"+NRefDetail(\$target)+":opener", \$actor);  
\}  
  
return TRUE;
      </Core:Property>
      <Core:Property property="merry:react-pre:open-what">
         X[M] /* If they are opening a virtual room, check if it's their room */  
if(\$target) \{  
  \$exit = Get(\$this, "details:"+NRefDetail(\$target)+":exit:dest");  
  if(\$exit \&\& NRefOb(\$exit).isvirtual) \{     
    if(!Get(\$actor, "virtualhome:"+NRefOb(\$exit).isvirtual)) \{  
      EmitTo(\$actor, Describe(\$target)+" is locked.");  
      return FALSE;  
    \}  
  \}  
\}  
return TRUE;
