      <Core:Property property="merry:react-post:examine-what">
         X[M] return TRUE;

if(sizeof(\$what) \> 1) return TRUE;

if(NRefDetail(\$what[0]) != "ground") return TRUE;

\$room = NRefOb(\$what[0]);

\$footprintsMap = prefixed_map(copy(\$room."*"), "footprints");

\$footprintExits = map_indices(\$footprintsMap);

if(!sizeof(\$footprintExits)) return TRUE;

for(\$i = 0; \$i \< sizeof(\$footprintExits); \$i++) \{
    \$footprints = Get(\$room, \$footprintExits[\$i]);

    \$direction = replace_strings(\$footprintExits[\$i], "exit-", "", "footprints:", "");

    \$spotted = (\{ \});
    for(\$j = 0; \$j \< sizeof(\$footprints); \$j++) \{
        if(\$footprints[\$j] != nil \&\& \$footprints[\$j]."trait:objecttype") \{
            \$spotted \|= (\{ \$footprints[\$j]."trait:objecttype" \});
        \}
    \}

    if(sizeof(\$spotted)) \{
        EmitTo(\$actor, "You spot " + common::string_list(\$list: \$spotted) + " tracks leading " + \$direction + ".");
    \}
\}

return TRUE;
      </Core:Property>
