      <Core:Property property="merry:react:enter-from%find_bombs">
         X[M] /* Pre-empt the rest for now */
return TRUE;

/* Do the PCs find some bombs? */

/* can only find one a day */
if(Int(\$actor."vandal:sewersearch:found")+60*60*16 \> time()) return TRUE;

/* They need to stay in the room for a little while */
\$delay(1, TRUE, "b50b");

/* Are they still in the room?
if(this != \$actor."base:environment") return TRUE;

/* Have they already checked this room? */
if(!\$actor."vandal:sewersearch") \$actor."vandal:sewersearch" = ([ ]);

if(!\$actor."vandal:sewersearch"[this] \|\| \$actor."vandal:sewersearch"[this]+60*60*40 \< time()) \{
  /* Haven't checked this room recently. Do a skill roll to see if they find the box */
  \$roll = ironclaw::roll(\$roller: \$actor, \$skills: (\{ "mental:observation", "academics:streetwise", "career:buccaneer", "career:pirate" \}));
  if(\$roll \> random(14)+10) \{
    \$item = Spawn(\$\{Ironclaw:Staff:Eos:Props:MuddyChest\});
    \$item."base:environment" = \$actor;
    if(\$item."base:environment" == \$actor) \{
      EmitTo(\$actor, "Something catches your eye. You reach into the muck, and pull out "+Describe(\$item, \$actor, nil, STYLE_NONPOSS)+".");
      EmitIn(\$actor."base:environment", Describe(\$actor)+" stops for a moment, reaches into the muck, and pulls out "+Describe(\$item, \$actor, nil, STYLE_NONPOSS)+".", \$actor);
      \$actor."vandal:sewersearch:found" = time();
    \} else \{
      /* They couldn't hold it. Were carrying too many other things. */
      Slay(\$item);
    \}
  \}
  \$actor."vandal:sewersearch"[this] = time();
\}

return TRUE;
