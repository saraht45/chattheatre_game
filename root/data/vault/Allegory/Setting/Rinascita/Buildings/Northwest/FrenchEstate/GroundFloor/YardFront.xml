         X[M] /* moving between partitions */    
    
/* are they going through the gate? */    
if((\$startpartition == "in the front yard" \&\& \$targetpartition == nil) \|\| (\$startpartition == nil \&\& \$targetpartition == "in the front yard")) \{    
  /* moving between the street and garden. is the gate closed? */    
  if(\$actor."base:environment"."details:exit-gate:exit:closed") \{    
    /* the gate is closed. Give a message */    
    EmitTo(\$actor, Describe(NewNRef(\$actor."base:environment", "exit-gate"))+" is currently closed.");    
    return FALSE;    
  \} else \{    
    /* the gate is open. Pass through. */    
    EmitTo(\$actor, "You step through "+Describe(NewNRef(\$actor."base:environment", "exit-gate"))+".");    
    EmitIn(\$actor."base:environment", Describe(\$actor)+" steps through "+Describe(NewNRef(\$actor."base:environment", "exit-gate"))+".", \$actor);    
    return TRUE;    
  \}    
\}    
    
    
