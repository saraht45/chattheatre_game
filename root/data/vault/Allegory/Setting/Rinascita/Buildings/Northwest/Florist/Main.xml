      <Base:Detail id="stained-glass-windows" prime="false" def="true" abstract="false" hidden="false" luminosity="0" targetable_in_dark="false" plural="false">
        <Base:Preposition close="true" against="false" under="true" on="false" inside="false" near="true" over="false" behind="false" before="true" beside="true"/>
        <Base:Names>
