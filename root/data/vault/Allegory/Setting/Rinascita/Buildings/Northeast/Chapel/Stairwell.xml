      <Core:Property property="merry:react:enter-into">
         X[M] if(NRefDetail(\$what) != "exit-up") \{ 
    return TRUE; 
\} 
 
if(!\$actor.guild) \{ 
    EmitTo(\$actor, "A priest stops you to let you know that only members of the chapel are authorized to enter there."); 
    return FALSE; 
\} 
 
if(\$actor.guild != \$\{Allegory:Guilds:church\}) \{ 
    EmitTo(\$actor, "A priest stops you to let you know that only members of the chapel are authorized to enter there."); 
    return FALSE; 
\} 
 
return TRUE;
