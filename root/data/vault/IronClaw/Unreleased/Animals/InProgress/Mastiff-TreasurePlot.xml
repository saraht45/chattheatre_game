      <Core:Property property="merry:act:start">
         X[M] \$this."trait:eyecolor" = oneof( "grey", "gray", "tan", "dun", "pearly", "ashen", "mahogany", "fawn", "ecru", "tan", "russet", "maroon", "auburn", "hazel", "crimson", "scarlet", "vermilion", "magenta", "verdant", "lavender", "mauve", "cerulean", "blue", "green", "red", "yellow", "teal", "aquamarine", "orange", "olive", "pink", "indigo", "violet", "purple" ); \$this."trait:color" = oneof( "grey", "gray", "tan", "dun", "pearly", "ashen", "mahogany", "fawn", "ecru", "tan", "russet", "maroon", "auburn", "hazel", "crimson", "scarlet", "vermilion", "magenta", "verdant", "lavender", "mauve", "cerulean", "blue", "green", "red", "yellow", "teal", "aquamarine", "orange", "olive", "pink", "indigo", "violet", "purple" );   
\$this."trait:spikes" = oneof( "grey", "gray", "tan", "dun", "pearly", "ashen", "mahogany", "fawn", "ecru", "tan", "russet", "maroon", "auburn", "hazel", "crimson", "scarlet", "vermilion", "magenta", "verdant", "lavender", "mauve", "cerulean", "blue", "green", "red", "yellow", "teal", "aquamarine", "orange", "olive", "pink", "indigo", "violet", "purple" );   
return TRUE;
      </Core:Property>
      <Core:Property property="merry:lib:commands">
         X[M] /* If the actor gave a command */         
if(\$evoke) \{         
         
  /* Resolve any weird variables that might be used */         
  if(\$(iob-2) \&\& !\$iob)         
    \$iob = \$(iob-2);         
         
  /* remove the quote marks from the command */         
  \$command = lower_case(\$evoke[1..strlen(\$evoke)-2]);         
  /* If we have any punctuation at the end of the command, remove it */         
  if((\$command[strlen(\$command)-1] \< 97) \|\| (\$command[strlen(\$command)-1] \> 122))         
    \$command = \$command[0..strlen(\$command)-2];         
         
  if((\$command == "follow") \|\| (\$command == "heel")) \{         
    EmitTo(\$actor, Describe(this)+" will now follow you.");         
    this.follow = \$actor;         
    Social(this, "step", nil, nil, "to", \$actor );         
  \} else if(\$command == "stay") \{         
    EmitTo(\$actor, Describe(this)+ "will no longer follow you.");         
    this.follow = nil;         
    Social(this, "whine", "ingratiatingly" );         
  \} else if(\$command == "sit") \{         
    this."base:stance" = 4;         
    EmitIn(this."base:environment", Describe(this)+" sits down.");         
  \} else if(\$command == "talk") \{         
    Social(this, "bark" );         
  \} else if(\$command == "bite") \{         
    if(\$iob) \{         
      Social(this, "bite", "viciously", nil, "into", \$iob);         
    \} else \{         
      Social(this, "bite");         
    \}         
  \} else if(\$command == "crouch") \{         
    this."base:stance" = 1;         
    EmitIn(this."base:environment", Describe(this)+" crouches.");         
  \} else if(\$command == "leap") \{         
    if(\$iob) \{         
      Social(this, "leap", "suddenly", nil, "at", \$iob);         
    \} else \{         
      Social(this, "leap");         
    \}         
  \} else if((\$command == "growl") \|\| (\$command == "kill")) \{         
    if(\$iob) \{         
      Social(this, "growl", "cruelly", nil, "at", \$iob);         
    \} else \{         
      Social(this, "growl");         
    \}         
  \} else if(\$command == "dismiss") \{
      EmitIn(\$actor."base:environment", Describe(this) + " runs off.");
      \$this."base:environment" = nil;
  \} else if(\$command=="commands") \{         
    EmitTo(\$actor, "Valid controls for "+Describe(this)+" are: follow, sit, dismiss, stay, heel, talk, bite, crouch, leap, and growl.");         
  \}         
\}         
return FALSE;
      </Core:Property>
      <Core:Property property="merry:react-post:command-dob">
         X[M] if (\$actor == Get(this, "trait:master"))         
\{         
return Call(\$this, "commands");         
\} else \{         
  Social(this, "growl", "viciously", nil, "at", \$actor);         
\}         
return TRUE;
      </Core:Property>
      <Core:Property property="merry:react-post:name-dob">
         X[M] if(!this."trait:master" \|\| (\$actor != this."trait:master"))         
  return TRUE;         
         
if(this."trait:name") \{         
  EmitTo(\$actor, Describe(this)+" already has a name. Sheesh.");         
  return TRUE;         
\}         
         
\$name = capitalize(\$evoke[1..strlen(\$evoke)-2]);         
/* If we have any punctuation at the end of the command, remove it */         
if(!((\$name[strlen(\$name)-1] \>= 97) \&\& (\$name[strlen(\$name)-1] \<= 122)) \&\& !((\$name[strlen(\$name)-1] \>= 65) \&\& (\$name[strlen(\$name)-1] \<= 90)))         
  \$name = \$name[0..strlen(\$name)-2];         
/* Only take the first word of the name */         
sscanf(\$name, "%s %*s", \$name);         
         
/* Tell the actor that this was successful */         
EmitTo(\$actor, Describe(this)+" will now respond to "+\$name+".");         
         
/* Set the name */         
this."trait:name" = \$name;         
/* set the sname */         
Set(this, "details:default:sname:"+lower_case(\$name), 1);         
this."details:default:definite:local" = 1;         
         
return TRUE;
      </Core:Property>
