      <Core:Property property="merry:react-desc:pull-dob">
         X[M] if(\$this."base:environment" != \$actor) return TRUE;
if(sizeof(\$dob) != 1) return TRUE;


\$delay(2, TRUE, "2cc2");   
   
switch(random(5)) \{   
  case 0:   
  \{   
    EmitIn(\$actor."base:environment", TAG(capitalize(Describe(\$this)) + " jerks and clacks in a dance of motion as " + Str(\$actor."base:pronoun") + " pulls the strings.", "social:third"));   
    break;   
  \}   
  case 1:   
  \{   
EmitIn(\$actor."base:environment", TAG(capitalize(Describe(\$this)) + " raises its arms in a wobbling pattern before dropping them back down as " + Str(\$actor."base:pronoun") + " pulls the strings.", "social:third"));   
    break;  
  \}   
  case 2:   
  \{   
EmitIn(\$actor."base:environment", TAG(capitalize(Describe(\$this)) + " makes an almost mechanical walking motion before going still once more as " + Str(\$actor."base:pronoun") + " pulls the strings.", "social:third"));   
    break; 
  \}   
  case 3:   
  \{   
EmitIn(\$actor."base:environment", TAG(capitalize(Describe(\$this)) + "'s arms and legs move in chaotic circles, the wood making a hollow clacking sound as " + Str(\$actor."base:pronoun") + " pulls the strings.", "social:third"));   
    break; 
  \}   
  default:   
  \{   
EmitIn(\$actor."base:environment", TAG(capitalize(Describe(\$this)) + " bobbles its head and shakes up, down, and all around as " + Str(\$actor."base:pronoun") + " pulls the strings.", "social:third"));   
    break;  
  \}   
\}   
   
return TRUE;
