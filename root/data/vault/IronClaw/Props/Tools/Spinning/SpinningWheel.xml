            <Base:PName pname="hooks"/>
          </Base:PNames>
        </Base:Names>
        <Base:Adjectives/>
        <Base:Descriptions>
          <Base:Description type="brief">
             the flyer's hooks
          </Base:Description>
          <Base:Description type="look">
             Four shiny brass hooks line each arm of the flyer. The hooks are used to store the yarn evenly on the bobbin.
          </Base:Description>
        </Base:Descriptions>
        <Base:Exit door="false" never_obvious="false" obvious_when_open="false" hidden_when_closed="false" separate="false" self-locking="false" deadbolt="false"/>
