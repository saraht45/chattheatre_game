      <Core:Property property="merry:react-post:rattle-dob">
         X[M] if(\$actor != \$this."base:environment") \{ 
  EmitTo(\$actor, "Totally uncool. You can rattle it to no effect."); 
  return FALSE; 
\} 
/* shortcuts for descriptions */ 
/* this is the actor */ 
\$me = Describe(\$actor); 
 /* grab our location */ 
\$env = \$actor."base:environment"; 
/* this produces him or her dependent on gender - third person */ 
\$mine = \$actor."trait:objective"; 
/* this is the staff in first person */ 
\$staff1 = Describe(\$this, nil, \$actor); 
/* this is the staff in third person */ 
\$staff3 = \$staff3 = Describe(\$this, nil, nil, STYLE_NONPOSS); 
/* Let's grab some extra details for fun */ 
\$heads = NewNRef(\$this, "heads"); 
\$heads = Describe(\$heads, nil, nil, STYLE_NONPOSS); 
\$ribbons = NewNRef(\$this, "ribbons"); 
\$ribbons = Describe(\$ribbons, nil, nil, STYLE_NONPOSS); 
/* let's mix up the verb a bit */ 
\$action = oneof( "shake", "rattle", "jangle", "jingle", "strangle" ); 
/* adverb time */ 
\$adverb = oneof( "wildly", "exuberantly", "testingly", "ominously", "playfully" ); 
/* sends the sound in what way */ 
\$send = oneof( "emits", "produces", "throws out"); 
/* what kind of noise */ 
\$decibel = oneof( "an ominous", "a loud", "an irritating", "a disturbing" ); 
/* the noise itself */ 
\$noise = oneof( "clacking", "jangling", "knocking", "jangling", "clammering" ); 
/* as the heads do what */ 
\$headact = oneof( "knock against each other.", "bang into one another.", "hit against the shaft of the staff.", "flail wildly" ); 
/* let's make random fun sentences for the end of it */ 
\$chance = random(6)+1;        
if(\$chance == 1) \{        
\$fun = "A trickle of bone dust falls to the ground.";        
\} else if(\$chance == 2) \{        
\$fun = "A glowing flame in each eye socket spins crazily.";        
\} else if(\$chance == 3) \{        
\$fun = "The ribbons flare outward on an unseen wind.";        
\} else if(\$chance == 4) \{        
\$fun = "The jawbone of each head opens and closes mutely.";        
\} else if(\$chance == 5) \{        
\$fun = "The staff twists against the hand holding it like a trapped snake.";        
\} else if(\$chance == 6) \{        
\$fun = "The shaft of the staff glows a bright blue momentarily.";        
\} else \{        
\$fun = "Absolutely nothing out of the ordinary occurs. How boring!"; 
\} 
 
/* build the emits using all the variables we have at our disposal */ 
/* add as many as you want to */ 
\$a = " and it " +\$send+ " " +\$decibel+ " " +\$noise+ " as " +\$heads+ " " +\$headact+"."; 
\$b = " causing " +\$ribbons+ " to spin " +\$adverb+ "."; 
 
/* now we pick one, add an entry for every emit you previously built */ 
\$msg = oneof( \$a, \$b ); 
 
/* send it out */ 
/* first person emit */ 
\$emit1 = "You " +\$action+ " " +\$staff1+ " " +\$msg; 
/* third person emit */ 
\$emit3 = \$me + " " +\$action+ "s " +\$staff3+ " " +\$msg; 
EmitTo(\$actor, \$emit1); 
EmitIn(\$env, \$emit3, \$actor); 
 
EmitTo(\$actor, \$fun); 
EmitIn(\$env, \$fun, \$actor); 
/* always give a return value for sanity's sake */ 
return TRUE;
      </Core:Property>
